#include<iostream>
#include<vector>
using namespace std;
int main(){
  // target sum of the triplets
  int n;
  cout<<"enter the number of the elements you want to enter in the array :: ";
  cin>>n;
  int arr[n];
  int target;
  cout<<"enter the target sum of the triplets :: ";
  cin>>target;
  for(int i=0;i<n;i++){
    cout<<"enter the elemets for the "<<i<<" th index value ::";
    cin>>arr[i];
  }
  int count=0;
  for(int i=0;i<n;i++){
    for(int j=i+1;j<n;j++){
      for(int k=j+1;k<n;k++){
        if(arr[i]+arr[j]+arr[k]==target) count++;
      }
    }
  }
  cout<<count;
  
   return 0;
}
