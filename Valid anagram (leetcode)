class Solution {
public:
    bool isAnagram(string s, string t) {
       /* anagram is basically the count
        of the frequency of the numbers */
        if(s.length()!=t.length()) return false;
        int freq[26]={0};
        for(int i=0;i<s.length();i++){
            freq[s[i]-'a']++;
            freq[t[i]-'a']--;
        }
            // now if the string s has more character then +ve otherwiswe -ve 
            for(int j=0;j<26;j++){
                if(freq[j]!=0) return false;
            }
            return true;
        }
};
