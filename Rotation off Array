#include<iostream>
#include<vector>
using namespace std;
int main(){
   // rotate any array by k  steps
   int n,rot,j=0;
   cout<<"enter how many numbers you want to enter in the array : ";
   cin>>n;
   int arr[n];
   for(int i=0;i<n;i++){
      cout<<"enter the element for the "<<i+1<<" th position : ";
      cin>>arr[i];
   }
   cout<<"enter the number of the rotation : ";
   cin>>rot;
   int brr[n];
   for(int g=0;g<rot;g++){
      brr[j++]=arr[n-rot+g];
   }
   for(int i=0;i<=n-rot;i++){
      brr[j++]=arr[i];
   }
   // finally answer is : 
   for(int p=0;p<n;p++){
      cout<<brr[p]<<" ";
   }
      return 0;
}
