class Solution {
public:
    bool isPalindrome(int x) {
        // palindrome 
        if(x<0 || (x%10==0 && x!=0)) return false;
        int ogn=x;
        int rem=0;
        long long ans=0;
        while(x>0){
            rem=x%10;
            ans=ans*10+rem;
            x/=10;
        }
        if(ans==ogn) return true;
        else return false;
    }
};
